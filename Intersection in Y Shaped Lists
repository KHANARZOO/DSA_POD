class Solution {
    public Node intersectPoint(Node head1, Node head2) {
        if(head1 == null || head2== null) return null;
        HashSet<Node> set = new HashSet<>();
        Node temp = head1;
        while(temp != null) {
            set.add(temp);
            temp = temp.next;
        }
        temp = head2;
        while(temp != null){
            if(set.contains(temp)) return temp;
            temp = temp.next;
        }
        return null;
    }
}
